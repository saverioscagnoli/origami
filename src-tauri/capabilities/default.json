{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "copy"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-start-dragging",
    "window:allow-minimize",
    "window:allow-toggle-maximize",
    "window:allow-close",
    "webview:allow-webview-close",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default"
  ]
}
